{
  "id": "http-server",
  "args": [],
  "cpus": 0.1,
  "mem": 64.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nginx",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "servicePort": 9000,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "hostPort": 0,
          "servicePort": 9443,
          "protocol": "tcp"
        }
      ]
    }
  },
  "labels": {
    "version": "v2"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
